                    android:layout_weight="1"
                    android:text="-"
                    android:textAlignment="center" />

            </TableRow>

            <TableRow
                android:background="#F0F7F7"
                android:padding="5dp">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="EMAIL"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="-"
                    android:textAlignment="center" />


            </TableRow>

            <TableRow
                android:background="#F0F7F7"
                android:padding="5dp">

